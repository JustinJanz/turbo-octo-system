# PostgreSQL connection
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Server
PORT=3000
NODE_ENV=development

# Security
# Comma-separated list of valid API keys: key:tenantId pairs
# e.g. "secret-key-1:tenant_acme,secret-key-2:tenant_globex"
API_KEYS=your-api-key-here:tenant_default

# Query execution limits
QUERY_TIMEOUT_MS=5000
MAX_ROWS=1000

# Rate limiting (requests per window per tenant)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=100
